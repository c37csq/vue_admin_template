{
  "name": "vue_admin_template",
  "version": "1.0.0",
  "author": {
    "name": "c37csq",
    "email": "1903498134@qq.com"
  },
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "serve": "npm run dev",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint --cache --max-warnings 0  \"src/**/*.{vue,ts,tsx}\" --fix",
    "prepare": "husky install",
    "lint:prettier": "prettier --write  \"src/**/*.{js,json,ts,scss,tsx,css,tsx,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^0.2.7",
    "axios": "^0.24.0",
    "crypto-js": "^4.1.1",
    "dayjs": "^1.10.7",
    "echarts": "^5.1.1",
    "element-plus": "^2.0.4",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.0-rc.10",
    "qs": "^6.10.3",
    "vue": "^3.2.16",
    "vue-i18n": "^9.1.9",
    "vue-router": "^4.0.12",
    "vue-types": "^4.1.1",
    "xlsx": "^0.17.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/crypto-js": "^4.0.2",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^16.11.11",
    "@types/nprogress": "^0.2.0",
    "@types/qs": "^6.9.7",
    "@types/mockjs": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vitejs/plugin-legacy": "^1.7.1",
    "vite-plugin-mock": "^2.9.6",
    "@vitejs/plugin-vue": "^1.9.3",
    "@vitejs/plugin-vue-jsx": "^1.3.7",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-define-config": "^1.2.5",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "esno": "^0.14.1",
    "fs-extra": "^10.0.1",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.2",
    "postcss": "^8.4.7",
    "postcss-html": "^1.3.0",
    "prettier": "^2.5.1",
    "sass": "^1.44.0",
    "stylelint": "^14.5.3",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.4.3",
    "unplugin-auto-import": "^0.5.1",
    "unplugin-vue-components": "^0.17.5",
    "vite": "^2.6.4",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-imagemin": "^0.4.6",
    "vite-plugin-svg-icons": "1.0.5",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vite-plugin-windicss": "^1.5.4",
    "vue-eslint-parser": "^8.3.0",
    "vue-tsc": "^0.3.0",
    "windicss": "^3.2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c37csq/vue_admin_template.git"
  },
  "homepage": "https://github.com/c37csq/vue_admin_template",
  "engines": {
    "node": "^12 || >=14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
